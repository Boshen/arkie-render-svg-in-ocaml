{
  "name": "@arkie/svg-renderer",
  "version": "0.0.9",
  "description": "Render arkie tree to svg string",
  "sideEffects": false,
  "scripts": {
    "test": "ava",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "build_all": "yarn build && webpack --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "cnpm": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.arkie.cn/arkie/svg-renderer"
  },
  "author": "Boshen",
  "license": "MIT",
  "main": "./dist/bundle.js",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@glennsl/bs-json": "^1.1.2",
    "ava": "1.0.0-beta.4",
    "browser-env": "^3.2.5",
    "bs-fetch": "^0.2.1",
    "bs-platform": "^2.2.3",
    "esm": "^3.0.18",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.8",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
